@page
@model ProyectoCiclo3.App.Frontend.Pages.FormServicioModel
@{
}

 
<div class = "row mt-5">
<div class="text-center col-6">
<form method="post">
  <label for="id" class="form-label">Id:</label>
  <input type="number" class="form-control" id="id" name="id" placeholder="Id">
  <label for="origen" class="form-label">Origen:</label>
  <input type="text" class="form-control" id="origen" name="origen" placeholder="Datos Origen">
  <label for="destino" class="form-label">Destino:</label>
  <input type="text" class="form-control" id="destino" name="destino" placeholder="Datos Destino">
  <label for="fecha" class="form-label">Fecha:</label>
  <input type="Date" class="form-control" id="fecha" name="fecha" placeholder="Fecha">
  <label for="hora" class="form-label">Hora:</label>
  <input type="time" class="form-control" id="hora" name="hora" placeholder="Hora">
  <label for="encomienda" class="form-label">Encomienda:</label>
  <input type="text" class="form-control" id="encomienda" name="encomienda" placeholder="Datos Encomienda">
  <hr>
  <button type="submit" class="btn btn-success">Enviar</button>
</form>
</div>
</div>